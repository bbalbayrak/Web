ccept=\".pdf\"\r\n                  placeholder=\"Teknik Çizim (PDF)\"\r\n                  onChange={(e) => setNewTechnicalDrawing(e.target.files[0])}\r\n                />\r\n              </td>\r\n              <td>\r\n                <input\r\n                  type=\"file\"\r\n                  accept=\".pdf,.jpg,.jpeg\"\r\n                  placeholder=\"Kılavuz (PDF, JPG)\"\r\n                  onChange={(e) => setNewGuide(e.target.files[0])}\r\n                />\r\n              </td>\r\n              <td>\r\n              <button\r\n                  onClick={() => {\r\n                    const newProduct = {\r\n                      name: newName,\r\n                      odooid: newOdooid,\r\n                      customer: newCustomer,\r\n                    };\r\n                    handleAddProduct(newProduct, newTechnicalDrawing, newGuide);\r\n                  }}\r\n                >\r\n                  <FontAwesomeIcon icon={faSave} />\r\n                  {' Kaydet'}\r\n                </button>\r\n                <button onClick={() => setAddingProduct(false)}>\r\n                  <FontAwesomeIcon icon={faTrash} />\r\n                  {' İptal'}\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          )}\r\n          \r\n          {products.map((product, index) => (\r\n  <tr key={index}>\r\n    <td>{product.name}</td>\r\n    <td>{product.odooid}</td>\r\n    <td>{product.customer}</td>\r\n    <td>\r\n      {product.technicaldrawingurl ? (\r\n        <a href={product.technicaldrawingurl} target=\"_blank\" rel=\"noopener noreferrer\">\r\n          {product.technicaldrawingurl}\r\n        </a>\r\n      ) : (\r\n        ''\r\n      )}\r\n    </td>\r\n    <td>\r\n      {product.guideUrl ? (\r\n        <a href={product.guideUrl} target=\"_blank\" rel=\"noopener noreferrer\">\r\n          {product.guideUrl}\r\n        </a>\r\n      ) : (\r\n        ''\r\n      )}\r\n    </td>\r\n    <td>\r\n      <button onClick={() => handleUpdateProduct(product.id, product)}>\r\n        <FontAwesomeIcon icon={faSave} />\r\n        {' Kaydet'}\r\n      </button>\r\n      <button onClick={() => handleDeleteProduct(product.id)}>\r\n        <FontAwesomeIcon icon={faTrash} />\r\n        {' Sil'}\r\n      </button>\r\n    </td>\r\n  </tr>\r\n))}\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ProductTable;\r\n","C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\api.js",[],[],"C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\components\\Works\\worksapi.js",[],[],"C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\components\\RouteGuard\\RouteGuard.js",[],[],"C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\components\\Users\\Users.js",[],[],"C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\components\\Users\\Userapi.js",[],[],"C:\\Users\\AlperenAlihanEr\\portal-test\\client\\src\\components\\Users\\CreateUser.js",["182"],[],{"ruleId":"183","severity":1,"message":"184","line":7,"column":27,"nodeType":"185","messageId":"186","endLine":7,"endColumn":33},{"ruleId":"183","severity":1,"message":"187","line":7,"column":42,"nodeType":"185","messageId":"186","endLine":7,"endColumn":46},{"ruleId":"183","severity":1,"message":"188","line":8,"column":8,"nodeType":"185","messageId":"186","endLine":8,"endColumn":19},{"ruleId":"183","severity":1,"message":"189","line":10,"column":8,"nodeType":"185","messageId":"186","endLine":10,"endColumn":19},{"ruleId":"183","severity":1,"message":"190","line":3,"column":35,"nodeType":"185","messageId":"186","endLine":3,"endColumn":41},{"ruleId":"191","severity":1,"message":"192","line":36,"column":9,"nodeType":"193","endLine":42,"endColumn":11},{"ruleId":"191","severity":1,"message":"192","line":46,"column":11,"nodeType":"193","endLine":50,"endColumn":13},{"ruleId":"183","severity":1,"message":"194","line":93,"column":13,"nodeType":"185","messageId":"186","endLine":93,"endColumn":28},{"ruleId":"183","severity":1,"message"